<p id="notice"><%= notice %></p>

<h1>Cultures</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Type</th>
      <th>Form</th>
      <th>Producer</th>
      <th>Product</th>
      <th>Attenuation min</th>
      <th>Attenuation max</th>
      <th>Alcohol tolerance</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cultures.each do |culture| %>
      <tr>
        <td><%= culture.name %></td>
        <td><%= culture.type %></td>
        <td><%= culture.form %></td>
        <td><%= culture.producer %></td>
        <td><%= culture.product_id %></td>
        <td><%= culture.attenuation_min %></td>
        <td><%= culture.attenuation_max %></td>
        <td><%= culture.alcohol_tolerance %></td>
        <td><%= link_to 'Show', culture %></td>
        <td><%= link_to 'Edit', edit_culture_path(culture) %></td>
        <td><%= link_to 'Destroy', culture, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Culture', new_culture_path %>
